{
  "name": "@mateothegreat/ts-kit",
  "version": "0.0.27",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./dist/*": {
      "types": "./dist/*.ts",
      "import": "./dist/*.ts"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "description": "typescript kit",
  "keywords": [
    "typescript",
    "kit",
    "utilities",
    "containers",
    "hierarchical"
  ],
  "author": {
    "name": "Matthew Davis",
    "email": "matthew@matthewdavis.io"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "release": "npm version from-git && npm publish --access public",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "dev:sync": "npm run build && npm link",
    "dev:sync:watch": "find . -name '*.ts' | entr -c npm run dev:sync"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "rxjs": "^7.8.2"
  }
}
