{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "ES2022"
  },
  "exclude": [
    "**/*.spec.ts"
  ],
  "include": [
    "./src/**/*"
  ]
}